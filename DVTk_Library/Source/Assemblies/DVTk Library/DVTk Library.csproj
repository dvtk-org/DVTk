IF EXIST "$(ProjectDir)..\..\..\..\..\Resources" SET  DVTK_LIBRARY_DIR=$(ProjectDir)..\..\..\..\..\

SET RESOURCES_DIR=%25DVTK_LIBRARY_DIR%25Resources\
