IF NOT EXIST "%25DESTINATION_DIR%25\CharacterSets" (
mkdir "%25DESTINATION_DIR%25\CharacterSets"
)

xcopy /y "%25RESOURCES_DIR%25CharacterSets" "%25DESTINATION_DIR%25\CharacterSets"
